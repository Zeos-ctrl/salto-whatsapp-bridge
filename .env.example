# Server port
PORT=3000

# WhatsApp target configuration
# For individual: just the phone number with country code (no + or spaces)
# Example: 1234567890
# For group: the full group ID from /list-chats endpoint
# Example: 123456789012345678@g.us
WHATSAPP_TARGET=1234567890

# Target type: 'individual' or 'group'
WHATSAPP_TARGET_TYPE=individual
